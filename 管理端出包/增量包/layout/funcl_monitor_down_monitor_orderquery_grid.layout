<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
  <div name="funcl_monitor_down_monitor_orderquery_grid">
     <img pos="0,0,-0,-0" image="skins\default\MsgBack.png" mode="extrude" framesize="0" show="1" response="0"/>
    <!--<img pos="1,0,-1,3" image="skins\default\line_blue.png" mode="tile" show="1" response="0"/>-->
    <!--<text crtext="808080" crmark="800000" align="left" pos="2,5,-15,25" title="  订单查询" font="big" show="1" />-->
   
    <gridctrl name="grid.funcl_monitor_down_monitor_orderquery_grid" pos="2,5,-2,-45" show="1"
        img-scroll="skin:IDB_SCROLL_V" img-scrollh="skin:IDB_SCROLL_H" img-sep="skin:IDB_LINE_RP"
        img-check="skin:IDB_CHECK_BOX" img-colsep=""
        img-count="8" image="skin:IDB_ORDER_STATUS_IMG"      
        left-pos="0" row-height="35" header-height="38" bk-transparent="10"
        font-title="big" font="default2" sort="1"
        valign="middle" down-row="1" align="center"  primary-key="order_no"  
        crtext="255,64,64,64" crhover="0,0,0" crpush="212,234,254" crhead="23,69,144" crrowhover="236,240,254">     
          <column title="序号" width="0" align="center" key="row_id" />   
        <column title="证券代码" width="100" align="center" key="stock_code" />
        <column title="证券名称" width="100" align="center" key="stock_name" />
        <column title="订单编号" width="100" align="center" key="order_no" />
        <column title="订单方向" width="100" align="center" key="order_dir" /> 
        <column title="订单价格" width="100" align="center" key="order_price" data_type="N"/>
        <column title="订单数量" width="100" align="right" key="order_qty" data_type="T"/>
        <column title="成交数量" width="100" align="right" key="execution_qty" data_type="T"/>
        <column title="成交金额" width="100" align="right" key="execution_amt" data_type="T"/>
        <column title="成交均价" width="100" align="center" key="execution_price" />    
        <column title="订单状态" width="100" align="center" key="order_status" />
        <column title="撤单数量" width="100" align="right" key="cancel_qty" data_type="T"/>
        <column title="订单时间" width="100" align="center" key="order_time" data_type="N"/>                    
        <column title="订单冻结金额" width="100" align="right" key="order_frozen_amt" data_type="T"/>
        <column title="全部费用" width="100" align="right" key="all_fare" data_type="T"/>
        <column title="产品名称" width="100" align="center" key="product_name" />
        <column title="交易单元名称" width="100" align="center" key="unit_name" />
    </gridctrl>
    <chkbtn name="monitor_down_checkbutton_all" align="left" title="全选" skin="IDB_CHECK_BOX" pos="2,-30" width="64" height="16" crtext="191919" show="1" check="false" showfocus="0"/>
    <chkbtn name="check_show_cancel" title="显示已撤销" skin="IDB_CHECK_BOX" pos="100,-30" width="100" height="15" crtext="e30202" show="1" check="false" showfocus="0"/>
	<chkbtn name="check_show_execution" title="显示已成交" skin="IDB_CHECK_BOX" pos="210,-30" width="100" height="15" crtext="0493a0" show="1" check="false" showfocus="0"/>
    <chkbtn name="check_show_cancancel" title="只显示可撤" skin="IDB_CHECK_BOX" pos="320,-30" width="100" height="15" crtext="174590" show="1" check="true" showfocus="0"/>
    <button name="monitor_down_stock_single_query" skin="IDB_BT_REFRESH" title="" pos="-180,-38,-100,-8" shortcut="RETURN" crtext="FFFFFF" />       
    <button name="monitor_down_button_revocation" skin="IDB_BT_cancellation" title="" pos="-90,-38,-10,-8" shortcut="RETURN" crtext="FFFFFF" />   
    
  </div>

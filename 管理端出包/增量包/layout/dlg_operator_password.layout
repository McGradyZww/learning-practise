<?xml version="1.0" encoding="UTF-8" standalone="no" ?>

<dlg name="dlg_operator_password" title="" width="350" height="300"  appwin="1" resize="0" frame="" bkimg="" crbk="3c5680">
	
	<base>
		<img pos="0,0,-0,30" image="skins\default\dlg_header.png" mode="titl" show="1" response="0" />
		<text name="title" crtext="FFFFFF" crmark="800000" crshadow="808080" font="default3" pos="50,5,200,25" title="密码修改" response="0" show="1" />
		<button name="button.close" pos="-34,0,-0,30" skin="IDB_BT_CLOSE" show="1" tip="关闭" shortcut="ESC" />
	</base>
	<body>
		<div name="dlg_operator_password.body" pos="0,30,-0,-0">
    	<img pos="0,0,-0,0" image="skins\default\dlg_bg.png" show="1" response="0" />
			<img pos="3,0,-3,-3" image="skins\default\MsgBack.png" mode="extrude" framesize="0" show="1" response="0"/>
			
			<!--操作员-->
			<text crtext="000000" crmark="800000" align="right" font="default3" pos="10,10,110,30" title="操作员：" response="0" show="1" />
			<text name="dlg_operator_password.operator_no" crtext="000000" crmark="800000" align="left" font="default3" pos="110,10,210,30" title="" response="0" show="1" />
			<text name="dlg_operator_password.operator_name" crtext="000000" crmark="800000" align="left" font="default3" pos="210,10,330,30" title="" response="0" show="1" />
			
			<!--原密码-->
			<text crtext="000000" crmark="800000" align="right" font="default3" pos="10,45,110,65" title="原密码：" response="0" show="1" />
			<edit name="dlg_operator_password.old_password" tabstop="1" skin="IDB_EDIT" pos="110,42" width="150" height="26" password="1" tip="原密码"
               multiline="0" number="0" readonly="0" maxchar="50" font="default" crtext="0,0,0" />
      <!--新密码-->
      <text crtext="000000" crmark="800000" align="right" font="default3" pos="10,80,110,100" title="新密码：" response="0" show="1" />
			<edit name="dlg_operator_password.new_password" tabstop="1" skin="IDB_EDIT" pos="110,77" width="150" height="26" password="1" tip="新密码"
               multiline="0" number="0" readonly="0" maxchar="50" font="default" crtext="0,0,0"/>
      <!--再次确认-->
      <text crtext="000000" crmark="800000" align="right" font="default3" pos="10,115,110,135" title="再次确认：" response="0" show="1" />
			<edit name="dlg_operator_password.new_password1" tabstop="1" skin="IDB_EDIT" pos="110,112" width="150" height="26" password="1" tip="再次输入新密码"
               multiline="0" number="0" readonly="0" maxchar="50" font="default" crtext="0,0,0"/>
			
			<!--确认、取消按钮-->
      <button name="button.ok" skin="IDB_BT_OK" title="[OK]" pos="-200,-45,-120,-15" font="default3" shortcut="RETURN" crtext="FFFFFF" defaultbutton="1"/>
	  	<button name="button.cancel" skin="IDB_BT_DEFAULT" title="[CANCEL]" pos="-110,-45,-30,-15" font="default3" show="1" />
		</div>
	</body>
</dlg>

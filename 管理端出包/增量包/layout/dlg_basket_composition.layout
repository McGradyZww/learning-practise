<?xml version="1.0" encoding="UTF-8" standalone="no" ?>

<dlg name="dlg_basket_composition" title="" width="850" height="400"  appwin="1" resize="0" frame="" bkimg="" crbk="3c5680">
	
	<base>
		<img pos="0,0,-0,35" image="skins\default\dlg_header.png" mode="extrude" show="1" response="0" />
		<text name="user.title" crtext="FFFFFF" crmark="800000" crshadow="808080" font="big"
			pos="30,4,200,25" title="[MSGTIP]" response="0" show="1" />
		<button name="button.close" pos="-34,0,-0,30" skin="IDB_BT_CLOSE" show="1" tip="关闭" shortcut="ESC" />
	</base>
	<body>
		<div name="container.body" pos="0,32,-0,-0">	
			<img pos="0,0,-0,0" image="skins\default\dlg_bg.png" show="1" response="0" />
			<img pos="3,0,-3,-3" image="skins\default\MsgBack.png" mode="extrude" framesize="0" show="1" response="0"/>
						
       <text crtext="000000" crmark="800000" align="left" pos="6,8,70,26" title="证券代码" font="default1" show="1" />
		   <editex name="basket_stock_code" tabstop="1" skin="IDB_EDIT" small-image="" pos="70,6" width="140" height="26" skinextern="IDB_EDITEX" leftwidth="75" crextern="255,255,255"
         multiline="0" autohscroll="1" autovscroll="1" number="0" readonly="0" maxchar="50"
         font="default1" crtext="0,0,0" changednotify="1"/>
       <text name="basket_weight_type" crtext="000000" crmark="800000" align="left" pos="215,8,279,26" title="成分数量" font="default1" show="1" />
       <edit name="stock_weight" tabstop="1" skin="IDB_EDIT" pos="279,6" width="70" height="26"
             multiline="0" autohscroll="1" autovscroll="1" number="0" readonly="0" maxchar="50"
             font="default1" crtext="0,0,0" />

       <text crtext="000000" crmark="800000" align="left" pos="354,8,418,26" title="替代证券" font="default1" show="1" />
		   <editex name="replace_stock_code" tabstop="1" skin="IDB_EDIT" small-image="" pos="413,6" width="140" height="26" skinextern="IDB_EDITEX" leftwidth="75" crextern="255,255,255"
         multiline="0" autohscroll="1" autovscroll="1" number="0" readonly="0" maxchar="50"
         font="default1" crtext="0,0,0" changednotify="1"/>
       <text crtext="000000" crmark="800000" align="left" pos="558,8,623,26" title="替代数量" font="default1" show="1" />
       <edit name="replace_stock_qty" tabstop="1" skin="IDB_EDIT" pos="623,6" width="70" height="26"
             multiline="0" autohscroll="1" autovscroll="1" number="0" readonly="0" maxchar="50"
             font="default1" crtext="0,0,0" />
       <button name="button_addstock" tabstop="1" skin="IDB_BT_OK" title="添 加" pos="-150,6,-80,32" shortcut="RETURN" crtext="ffffff" defaultbutton="1" font="default1"/>        
       <button name="button_delstock" tabstop="1" skin="IDB_BT_DEFAULT" title="删 除" pos="-76,6,-6,32" shortcut="RETURN" crtext="0" font="default1"/>        
               
 			 <img pos="4,35,-4,37" image="skins\default\line_split.png" mode="tile" show="1" />
     	 <gridctrl name="grid.Basketcomposition" pos="2,39,-2,-38" show="1"
        img-scroll="skin:IDB_SCROLL_V" img-scrollh="skin:IDB_SCROLL_H" img-sep="skin:IDB_LINE_RP"
        img-check="skin:IDB_CHECK_BOX" img-colsep=""
        img-count="6" image="skin:IDB_LIST_IMG"
        left-pos="0" row-height="35" header-height="38" bk-transparent="10"
        font-title="big" font="default2"
        valign="middle" down-row="1" align="center"
        crtext="255,64,64,64" crhover="0,0,0" crpush="212,234,254" crhead="23,69,144" crrowhover="236,240,254">
	        <column title="市场编号" width="0" align="center" key="exchange_no" />
	        <column title="证券代码" width="100" align="center" key="stock_code" />
	        <column title="证券名称" width="100" align="center" key="stock_name" />
	        <column title="成分数量" width="100" align="center" key="composition_qty" />
	        <column title=" 权  重 " width="100" align="center" key="composition_ratio" />
	        <column title="替代代码" width="100" align="center" key="replace_stock_code" />
	        <column title="替代名称" width="100" align="center" key="replace_stock_name" />
	        <column title="替代数量" width="100" align="center" key="replace_qty" />
        
    	 </gridctrl>
 			 <img pos="4,-36,-4,-34" image="skins\default\line_split.png" mode="tile" show="1" />
       <button name="button.ok" tabstop="1" skin="IDB_BT_OK" title="确 定" pos="-150,-32,-80,-6" shortcut="RETURN" crtext="ffffff" font="default1"/>        
       <button name="button.cancel" tabstop="1" skin="IDB_BT_DEFAULT" title="取 消" pos="-76,-32,-6,-6" shortcut="" crtext="0" font="default1"/>        
		</div>
		
	</body>
  
</dlg>

<?xml version="1.0" encoding="UTF-8" standalone="no" ?>

<dlg name="dlg_stockcommand_modifyprice" title="" width="900" height="600"  appwin="1" resize="0" frame="" bkimg="" crbk="3c5680">
	
	<base>
		<img pos="0,0,-0,30" image="skins\default\dlg_header.png" mode="titl" show="1" response="0" />
		<text name="title" crtext="FFFFFF" crmark="800000" crshadow="808080" font="default3"
			pos="50,5,200,25" title="指令修改" response="0" show="1" />
	</base>
	<body>
		<div name="modifyprice.body" pos="0,32,-3,-3">	
			<img pos="0,0,-0,-0" image="skins\default\dlg_bg.png" show="1" response="0"/>
		<img pos="3,0,-0,-0" image="skins\default\MsgBack.png" mode="extrude" framesize="0" show="1" response="0"/>
		<!---
		<chkbtn name="stockeorder_checkbutton_all" align="left" title="全选" skin="IDB_CHECK_BOX" pos="2,15" width="64" height="16" crtext="191919" show="1" check="true" showfocus="0"/>	
		---->
       <gridctrl name="grid.funcl_modifyprice_stockeorder_query" pos="10,5,-2,-65" show="1"
        img-scroll="skin:IDB_SCROLL_V" img-scrollh="skin:IDB_SCROLL_H" img-sep="skin:IDB_LINE_RP"
        img-check="skin:IDB_CHECK_BOX" img-colsep=""
        img-count="6" image="skin:IDB_LIST_IMG"      
        left-pos="30" row-height="35" header-height="38" bk-transparent="10"
        font-title="big" font="default2"  sort="1" header-check="1"
        valign="middle" down-row="1" align="center" primary-key="command_id" 
        crtext="255,64,64,64" crhover="0,0,0" crpush="212,234,254" crhead="23,69,144" crrowhover="236,240,254">
  		
  			<column title="指令编号" width="80" align="center" key="command_id" />
  			<column title="指令批号" width="80" align="center" key="batch_no" />
  		  <column title="产品名称" width="100" align="center" key="product_name" />  	
  			<column title="证券代码" width="60" align="center" key="stock_code" />
				<column title="证券名称" width="60" align="center" key="stock_name" /> 
					
  			<column title="指令限价" width="100" align="right" key="limit_price" data_type="N"/>
  			<column title="修改价格" width="100" align="right" key="modify_price" data_type="N"/>
  			<column title="修改数量" width="100" align="right" key="modify_qty" data_type="T"/>
				<column title="指令数量" width="100" align="right" key="command_qty" data_type="T"/>
				<column title="订单数量" width="100" align="right" key="order_qty" data_type="T"/>
				<column title="成交数量" width="100" align="right" key="execution_qty" data_type="T"/>
				<column title="指令方向" width="80" align="center" key="command_dir" />  	
				<column title="指令金额" width="100" align="right" key="command_amt" data_type="T"/>
				<column title="开始日期" width="100" align="center" key="command_begin_date" data_type="N"/>
				<column title="结束日期" width="100" align="center" key="command_end_date" data_type="N"/>
				<column title="开始时间" width="100" align="center" key="command_begin_time" data_type="N"/>
				<column title="结束时间" width="100" align="center" key="command_end_time" data_type="N"/>			
				
				<column title="单元名称" width="100" align="center" key="unit_name" />  
					<column title="" width="0" align="center" key="row_id" />     
		</gridctrl>
		<!--分割线-->
		<img pos="1,-63,-1,-60" image="skins\default\line_blue.png" mode="tile" show="1" response="0"/>   
		 <chkbtn name="stockeorder_check.price_all" align="left" title="同一代码同方向指令，价格联动修改" skin="IDB_CHECK_BOX" pos="10,-45" width="230" height="16" crtext="191919" show="1" check="true" showfocus="0" />
		     
				<!--有效起始时间-->   
			<text crtext="000000" crmark="800000" align="left" pos="240,-45" width="64" height="26" title="有效时间" font="default1" show="1" />
			 <nativewnd name="command_begin_time" skin="IDB_EDIT" pos="300,-45" width="160" height="26"
				multiline="0" autohscroll="1" autovscroll="1" number="0" readonly="0" maxchar="50"
	            font="default" crtext="0,0,0" show="1" />			
			
			<!--有效结束时间-->   
			<text crtext="000000" crmark="800000" align="left" pos="460,-45" width="20" height="26"  title="至" font="default1" show="1" />
			 <nativewnd name="command_end_time" skin="IDB_EDIT" pos="480,-45" width="160" height="26"
				multiline="0" autohscroll="1" autovscroll="1" number="0" readonly="0" maxchar="50"
	            font="default" crtext="0,0,0" show="1" />
					
			<button name="dlg_button.command.modifyprice" tabstop="0" skin="IDB_BT_DEFAULT" title="修改价格" pos="220,-50,320,-10" font="default3" shortcut="RETURN" show="0"  />     
		    <button name="dlg_button.command.modify" tabstop="0" skin="IDB_BT_OK" title="修改指令" pos="700,-50,790,-10" font="default3" shortcut="RETURN" show="1"  />      		
	  		<button name="button.cancel" tabstop="1" skin="IDB_BT_DEFAULT" title="取消" shortcut="ESC" pos="-70,-50,-10,-10" font="default3" show="1" />
		    
		</div>
		
	</body>
  
</dlg>

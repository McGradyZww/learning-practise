    <button name="iporeport_query_button.query" title="" pos="950,15" width="80" height="30" skin="IDB_BT_QUERY" tip="查询" show="1" />
    
   <!--结果集grid-->
    <img pos="1,57,-1,60" image="skins\default\line_blue.png" mode="tile" show="1" response="0"/>

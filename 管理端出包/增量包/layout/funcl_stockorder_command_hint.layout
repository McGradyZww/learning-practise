<!--下单确认框：多账户指令-->
<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<dlg name="funcl_ins_multicommand_hint" title="" width="840" height="550"  appwin="1" resize="0" frame="" bkimg="" crbk="3c5680">
	
	<base>
		<img pos="0,0,-0,30" image="skins\default\dlg_header.png" mode="titl" show="1" response="0" />
		<text name="title" crtext="FFFFFF" crmark="800000" crshadow="808080" font="default3"
			pos="50,5,200,25" title="订单下达确认" response="0" show="1" />
	</base>
	
	<body>
		<div name="funcl_stockorder_command_hint.body" pos="0,30,-0,-0">
			<img pos="0,0,-0,0" image="skins\default\dlg_bg.png" show="1" response="0" />
			<img pos="3,0,-3,-3" image="skins\default\MsgBack.png" mode="extrude" framesize="0" show="1" response="0"/>

      <img pos="10,2" width="36" height="36" image="skins\image\icon_info.png" mode="tile" show="1" response="0"/>
			<text crtext="000000" crmark="800000" align="left" pos="60,10" width="80" height="36" title="订单下达" font="default4" show="1"/>
			<text name="txtfail" crtext="FF0000" crmark="800000" align="right" pos="140,10" width="60" height="36" title="%d笔" font="default4" show="1" />
			<text name="txtsuccess" crtext="000000" crmark="800000" align="left" pos="200,10" width="200" height="36" title="失败，%d笔成功" font="default4" show="1"/>
			
			<!--指令信息汇总grid-->
			<gridctrl name="grid.stockorder_command_hint.total" pos="5,38,-5,116" show="1"
        img-scroll="skin:IDB_SCROLL_V" img-scrollh="skin:IDB_SCROLL_H" img-sep="skin:IDB_LINE_RP"
        img-check="" img-colsep=""
        img-count="" image="" sort="1" 
        left-pos="0" row-height="30" header-height="45" bk-transparent="100"
        font-title="bold3" font="default2" shortcut="SPACE"
        valign="middle" down-row="1" align="center" grid-tip="1"
        crtext="000000" crhover="000000" crpush="e1effc" crhead="00a0e9" crtitle="00a0e9" crrowhover="e1effc" >
				<column title="证券代码" width="80" key="stock_code" />
				<column title="证券名称" width="80" key="stock_name" />
				<column title="指令价格" width="100" key="command_price" data_type="N" />
				<column title="指令数量" width="80" key="command_qty" data_type="T" />
				<column title="指令金额" width="100" key="command_amt" data_type="T" />
				<!--column title="交易员" width="100" key="command_operator" /-->
			</gridctrl>
      
      <!--分割线-->
      <img pos="1,139,-1,142" image="skins\default\line_blue.png" mode="tile" show="1" response="0"/>
      
      <!--指令信息明细grid-->
			<gridctrl name="grid.stockorder_command_hint.detail" pos="5,146,-5,-62" show="1"
        img-scroll="skin:IDB_SCROLL_V" img-scrollh="skin:IDB_SCROLL_H" img-sep="skin:IDB_LINE_RP"
        img-check="" img-colsep="" sort="1" 
        img-count="2" image="skin:IDB_ORDERFLAG_LIST_IMG" sort="0" 
        left-pos="0" row-height="30" header-height="45" bk-transparent="100"
        font-title="bold3" font="default2" shortcut="SPACE"
        valign="middle" down-row="1" align="center" grid-tip="1"
        crtext="000000" crhover="000000" crpush="e1effc" crhead="00a0e9" crtitle="00a0e9" crrowhover="e1effc" >
        <column title="状态" width="40" key="order_status" />
        <column title="下达结果" width="240" key="order_result" />
				<column title="产品名称" width="120" key="product_name" />
				<column title="单元名称" width="120" key="unit_name" />
				<column title="订单方向" width="80" key="order_dir"  />
				<column title="订单数量" width="100" key="order_qty" data_type="T" />
				<column title="订单金额" width="120" key="order_amt" data_type="T" />
			</gridctrl>             
     	
     	<!--分割线-->
      <img pos="1,-63,-1,-60" image="skins\default\line_blue.png" mode="tile" show="1" response="0"/>
      
      <!--提示文字、确认、取消按钮-->
      <!--text crtext="000000" crmark="800000" align="left" pos="5,-45,255,-15" title="请确认是否下达本指令?" font="default4" show="1" /-->
      <button name="button.ok" tabstop="1" skin="IDB_BT_OK" title="[OK]" pos="-140,-50,-10,-10" font="default3" shortcut="RETURN" crtext="FFFFFF" show="1" defaultbutton="1"/>
	  	<!--button name="button.cancel" tabstop="1" skin="IDB_BT_DEFAULT" title="[CANCEL]" pos="-70,-50,-10,-10" font="default3" show="1" defaultbutton="1"/-->
		</div>
	</body>
</dlg>

		<text name="stockcodename" crtext="ffffff" crmark="800000" align="left" pos="65,8,145,25" title="--------" font="bold2" show="1" />
		<text name="lastprice" crtext="af0100" crmark="800000" align="left" pos="145,8,195,25" title="------" font="bold2" show="1" />
		<text name="increaseamount" crtext="af0100" crmark="800000" align="left" pos="200,8,260,25" title="------" font="bold2" show="1" />

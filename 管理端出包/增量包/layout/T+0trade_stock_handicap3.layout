        left-pos="0" row-height="23" header-height="30" bk-transparent="0"
        font-title="big" font="default2"
        valign="middle" down-row="0" align="right" readonly="1"
        crtext="000000" crhover="000000" crpush="e1effc" crhead="ffffff" crtitle="00a0e9" crrowhover="e1effc">
        <column title="时间" width="70" align="right" key="execution_time" />

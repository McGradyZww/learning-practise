<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
  <div name="funcl_stockeorder_query_grid_batchsum_tab">
    <img pos="0,0,-0,-0" image="skins\default\MsgBack.png" mode="extrude" framesize="0" show="1" response="0"/>
    <!--<img pos="1,0,-1,3" image="skins\default\line_blue.png" mode="tile" show="1" response="0"/>-->
    <!--<text crtext="808080" crmark="800000" align="left" pos="2,5,-15,25" title="  订单查询" font="big" show="1" />-->
   
    <gridctrl name="grid.funcl_stockeorder_batchsum_query" pos="2,5,-2,-45" show="1"
        img-scroll="skin:IDB_SCROLL_V" img-scrollh="skin:IDB_SCROLL_H" img-sep="skin:IDB_LINE_RP"
        img-check="skin:IDB_CHECK_BOX" img-colsep="skin:IDB_LINE_SPLIT"  gird-tip="1"
        img-count="6" image="skin:IDB_LIST_IMG"   sort="1"   
        left-pos="0" row-height="35" header-height="38" bk-transparent="10"
        font-title="big" font="default2"
        valign="middle" down-row="1" align="center"  primary-key="batch_no"  
        crtext="255,64,64,64" crhover="0,0,0" crpush="212,234,254" crhead="23,69,144" crrowhover="236,240,254">     
        <column title="批号" width="100" align="center" key="batch_no" />   
        <column title="证券代码" width="100" align="center" key="stock_code"  />
        <column title="证券名称" width="100" align="center" key="stock_name" />
        <column title="订单方向" width="100" align="center" key="order_dir" /> 
        <column title="订单价格" width="100" align="center" key="order_price" data_type="N" />
        <column title="订单数量" width="100" align="right" key="order_qty" data_type="T"  />
        <column title="成交数量" width="100" align="right" key="execution_qty" data_type="T" />
        <column title="成交金额" width="100" align="right" key="execution_amt" data_type="T" />
        <column title="成交均价" width="100" align="center" key="execution_price" data_type="N" /> 
        <column title="撤单数量" width="100" align="right" key="cancel_qty" data_type="T" />  
    </gridctrl>
    <chkbtn name="check_show_cancel" title="显示已撤销" skin="IDB_CHECK_BOX" pos="10,-30" width="100" height="15" crtext="e30202" show="1" check="false" showfocus="0"/>
	  <chkbtn name="check_show_execution" title="显示已成交" skin="IDB_CHECK_BOX" pos="120,-30" width="100" height="15" crtext="0493a0" show="1" check="false" showfocus="0"/>
          
    <button name="ins_trade_button.batchsum.revocation" skin="IDB_BT_STANDARD" title="撤单" font="default2" pos="-150,-38,-70,-8" />    
	<button name="ins_trade.stock.single.query" skin="IDB_BT_STANDARD" title="刷新" font="default2" pos="-60,-38,-10,-8" />     
  </div>

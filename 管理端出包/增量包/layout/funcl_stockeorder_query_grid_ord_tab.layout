<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
  <div name="funcl_stockeorder_query_grid_ord_tab">
    <img pos="0,0,-0,-0" image="skins\default\MsgBack.png" mode="extrude" framesize="0" show="1" response="0"/>
    <!--<img pos="1,0,-1,3" image="skins\default\line_blue.png" mode="tile" show="1" response="0"/>-->
    <!--<text crtext="808080" crmark="800000" align="left" pos="2,5,-15,25" title="  订单查询" font="big" show="1" />-->
   
    <gridctrl name="grid.funcl_stockeorder_query" pos="2,5,-2,-55" show="1"			
		img-scroll="skin:IDB_SCROLL_V" img-scrollh="skin:IDB_SCROLL_H" img-sep="skin:IDB_LINE_RP"
        img-check="skin:IDB_CHECK_BOX" img-colsep="skin:IDB_LINE_SPLIT" header-check="0" 
        img-count="8" image="skin:IDB_ORDER_STATUS_IMG" sort="1" editcolumn="1"
        left-pos="30" row-height="35" header-height="38" bk-transparent="1"
        font-title="big" font="default2" grid-tip="1"
        valign="middle" down-row="1" align="center" primary-key="order_no" 
        crtext="255,64,64,64" crhover="0,0,0" crpush="212,234,254" crhead="23,69,144" crrowhover="236,240,254">      
        <column title="证券代码" width="100" align="center" key="stock_code"  />
        <column title="证券名称" width="100" align="center" key="stock_name" />
        <column title="订单编号" width="100" align="center" key="order_no" data_type="N" />
        <column title="订单方向" width="100" align="center" key="order_dir" /> 
        <column title="订单价格" width="100" align="center" key="order_price" data_type="N" />
        <column title="订单数量" width="100" align="right" key="order_qty" data_type="T"  />
        <column title="成交数量" width="100" align="right" key="execution_qty" data_type="T" />
        <column title="成交金额" width="100" align="right" key="execution_amt" data_type="T" />
        <column title="成交均价" width="100" align="center" key="execution_price" data_type="N" /> 		
        <column title="订单状态" width="100" align="center" key="order_status" />		
        <column title="撤单数量" width="100" align="right" key="cancel_qty" data_type="T" />
        <column title="订单时间" width="100" align="center" key="order_time" data_type="N" />                    
        <column title="订单冻结金额" width="100" align="right" key="order_frozen_amt" data_type="T" />
        <column title="全部费用" width="100" align="right" key="all_fare" data_type="T" />		
        <column title="产品名称" width="100" align="center" key="product_name" />
        <column title="交易单元名称" width="100" align="center" key="unit_name" />
		<column title="废单原因" width="150" align="center" key="response_info"  />		
        <column title="序号" width="0" align="center" key="row_id" />   
    </gridctrl>
    <chkbtn name="stockeorder_checkbutton_all" align="left" title="全选" skin="IDB_CHECK_BOX" pos="2,-55" width="64" height="16" crtext="191919" show="0" check="false" showfocus="0"/>
    <chkbtn name="check_show_waste" title="废单" skin="IDB_CHECK_BOX" pos="2,-55" width="50" height="15" crtext="174590" show="1" check="true" showfocus="0"/>
    <chkbtn name="check_show_cancel" title="已撤销" skin="IDB_CHECK_BOX" pos="52,-55" width="65" height="15" crtext="e30202" show="1" check="false" showfocus="0"/>
	  <chkbtn name="check_show_execution" title="已成交" skin="IDB_CHECK_BOX" pos="120,-55" width="65" height="15" crtext="0493a0" show="1" check="false" showfocus="0"/>
    <chkbtn name="check_show_cancancel" title="只显示可撤" skin="IDB_CHECK_BOX" pos="190,-55" width="90" height="15" crtext="174590" show="1" check="true" showfocus="0"/>
     <button name="stockeorder_button.revocation" skin="IDB_BT_STANDARD" title="撤单" font="default2" pos="-150,-38,-70,-8" />
     <button name="ins_trade.stock.single.query" skin="IDB_BT_STANDARD" title="刷新" font="default2" pos="-60,-38,-10,-8" />       
   
     <!--证券代码-->
	   <edit name="stockeorder_query.stock_code" tabstop="1" skin="IDB_EDIT" pos="10,-30" width="64" height="25"
               multiline="0" autohscroll="1" autovscroll="1" number="0" readonly="0" maxchar="50" tip="证券代码"
               font="default" crtext="0,0,0" entrer_selectall="1"  />
	   <!--单元-->
	 	<combobox name="stockeorder_query.unit_code" tabstop="1" skin="IDB_EDIT" pos="80,-30" width="120" height="25" tip="交易单元"
				  small-image="skin:IDB_DROP_DOWN_small" autohscroll="1" autovscroll="1"
				  multiline="0" readonly="1" maxchar="50" list-height="100"
					font="default" crtext="0,0,0" crtip="128,128,128">
	  </combobox>
	  
	  <edit name="stockeorder_query.beginprice" tabstop="1" skin="IDB_EDIT" pos="205,-30" width="54" height="25"
               multiline="0" autohscroll="1" autovscroll="1" double="1" readonly="0" maxchar="50" tip="￥"
               font="default" crtext="0,0,0" entrer_selectall="1"  />
    <text crtext="000000" crmark="800000" align="left" pos="260,-30" width="10" height="25" title="~" font="default1" show="1" />		
	
		<edit name="stockeorder_query.endprice" tabstop="1" skin="IDB_EDIT" pos="270,-30" width="54" height="25"
               multiline="0" autohscroll="1" autovscroll="1" double="1" readonly="0" maxchar="50" tip="￥"
               font="default" crtext="0,0,0" entrer_selectall="1"  />

    <button name="stockeorder_button.query" title="" pos="350,-30" width="50" height="25" skin="IDB_BT_STANDARD" title="查询" tip="查询" show="1" />
    <button name="stockeorder_button.revocation" skin="IDB_BT_cancellation" title="" pos="-90,-35,-10,-10" show="0" crtext="FFFFFF" />        
     </div>

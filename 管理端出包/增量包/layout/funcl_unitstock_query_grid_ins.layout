<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
  <div name="funcl_unitstock_query_grid_ins">
    <img pos="0,0,-0,-0" image="skins\default\MsgBack.png" mode="extrude" framesize="0" show="1" response="0"/>
    <!--<img pos="1,0,-1,3" image="skins\default\line_blue.png" mode="tile" show="1" response="0"/>-->
    <!--<text crtext="808080" crmark="800000" align="left" pos="2,5,-15,25" title="  持仓查询" font="big" show="1" />-->
   
    <gridctrl name="grid.funcl_unitstock_query" pos="2,2,-2,-30" show="1"
        img-scroll="skin:IDB_SCROLL_V" img-scrollh="skin:IDB_SCROLL_H" img-sep="skin:IDB_LINE_RP"
        img-check="skin:IDB_CHECK_BOX" img-colsep=""
        img-count="6" image="skin:IDB_LIST_IMG" grid-tip="1"
        left-pos="0" row-height="35" header-height="38" bk-transparent="10"
        font-title="big" font="default2" sort="1" editcolumn="1" 
        valign="middle" down-row="1" align="center" bCellColor="0"
        crtext="255,64,64,64"  crhover="0,0,0" crpush="212,234,254" crhead="23,69,144" crrowhover="236,240,254">
        <column title="持仓序号" width="0" align="center" key="row_id"  deletable="0" />      
        <column title="交易单元名称" width="100" align="center" key="unit_name" /> 
        <column title="产品名称" width="100" align="center" key="product_name"  deletable="0" />      
        <column title="证券代码" width="64" align="center" key="stock_code" deletable="0" />
        <column title="证券名称" width="64" align="center" key="stock_name" />
        <column title="可用数量" width="100" align="center" key="command_available_qty" data_type="T" />
        <column title="当前数量" width="100" align="center" key="current_qty" data_type="T" />
        <column title="持仓市值" width="100" align="center" key="holding_amt" data_type="T" />
        <column title="浮动盈亏" width="100" align="center" key="float_pandl" data_type="T" />
        <column title="盈亏比例" width="100" align="center" key="pandl_ratio" data_type="N"/>
        <column title="当前价格" width="100" align="center" key="last_price" data_type="N" />
        <column title="成本价格" width="100" align="center" key="cost_price" data_type="N" />             
        <column title="当前成本金额" width="100" align="center" key="current_cost_amt" data_type="T" />
        <column title="实现盈亏" width="100" align="center" key="realize_pandl" data_type="N" />
        <column title="交易单元" width="100" align="center" key="unit_code"  deletable="0" /> 
        <column title="产品编码" width="100" align="center" key="product_code"  deletable="0" />   
        <column title="产品序号" width="100" align="center" key="product_id" />      
        <column title="成交占用数量" width="100" align="center" key="execution_capture_qty" data_type="T" />
        <column title="成交释放数量" width="100" align="center" key="execution_release_qty" data_type="T" />
        <column title="冻结数量" width="100" align="center" key="frozen_qty" data_type="T" />
        <column title="解冻数量" width="100" align="center" key="unfrozen_qty" data_type="T" />
        <column title=" 市  场 " width="100" align="center" key="exchange_no" />
        <column title="投资类型" width="100" align="center" key="invest_type" />
        <column title="期初数量" width="100" align="center" key="begin_qty" data_type="T" />
        <column title="昨收盘价" width="100" align="center" key="pre_close_price" data_type="N" />
        <column title="利息成本金额" width="100" align="center" key="interest_cost_amt" data_type="T" />
        <column title="利息收益" width="100" align="center" key="interest_pandl" data_type="T" />
        <column title="股东代码" width="100" align="center" key="stock_account"  deletable="0" />
        <column title="资产账户" width="100" align="center" key="asset_account"  deletable="0" />
        <column title="公司名称" width="100" align="center" key="company_name" />
    </gridctrl>
     <!--市场编号-->
	  <text crtext="000000" crmark="800000" align="right" pos="0,-23" width="64" height="25" title="市场编号:"  show="1"/>
	  <combobox name="stockeorder_query.exchange_no" tabstop="1" skin="IDB_EDIT" pos="65,-27" width="80" height="25" tip="市场编号"
				  small-image="skin:IDB_DROP_DOWN_small"
				  multiline="0" readonly="1" maxchar="50"
					font="default" crtext="0,0,0" crtip="128,128,128">
	  </combobox>
	   <!--证券代码-->
	  <text crtext="000000" crmark="800000" align="right" pos="145,-23" width="64" height="25" title="证券代码:"  show="1"/>
	  <edit name="stockeorder_query.stock_code" tabstop="1" skin="IDB_EDIT" pos="210,-27" width="80" height="25"
               multiline="0" number="0" readonly="0" maxchar="50" font="default" crtext="0,0,0" stockcode="1" changednotify="1" maxchar="6"/>
	 
	   <!--单元-->
	   <text crtext="000000" crmark="800000" align="right" pos="290,-23" width="64" height="25" title="交易单元:"  show="1"/>
    <combobox name="stockeorder_query.unit_code" tabstop="1" skin="IDB_EDIT" pos="355,-27" width="120" height="25" tip="交易单元"
				  small-image="skin:IDB_DROP_DOWN_small"  list-height="220"
				  multiline="0" readonly="1" maxchar="50"
					font="default" crtext="0,0,0" crtip="128,128,128">
	  </combobox>
	  
	 <button name="stockeorder_button.query" title="查询" pos="-150,-27" width="70" height="25" skin="IDB_BT_BIG" tip="查询" show="1" />
     <button name="unitstock_button.refresh" title="刷新" pos="-70,-27" width="50" height="25" skin="IDB_BT_BIG" tip="刷新" show="1" />
	  

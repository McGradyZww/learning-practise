        <column title="订单状态" width="100" align="center" key="order_status" />
        <column title="债券代码" width="100" align="center" key="stock_code"  />
		<column title="债券名称" width="100" align="center" key="stock_name"  />
        <column title="交易方向" width="100" align="center" key="order_dir" />
        <column title="提交数量" width="100" align="center" key="order_qty" data_type="T" />
		 <column title="产品" width="100" align="center" key="product_name" />
        <column title="交易单元" width="100" align="center" key="unit_name" />
		
        <column title="成交金额" width="0" align="center" key="execution_amt" data_type="T" />
        <column title="成交数量" width="0" align="center" key="execution_qty" data_type="T" />          
        <column title="撤单数量" width="0" align="center" key="cancel_qty" data_type="T" />
        <column title="废单原因" width="150" align="center" key="response_info"  />		
        <column title="资产账户" width="0" align="center" key="asset_account"  />                     
        <column title="批号" width="0" align="center" key="batch_no" data_type="N" />

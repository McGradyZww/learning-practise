        <column title="产品编码" width="0" align="center" key="product_code"   deletable="0" />         
    </gridctrl>
    
     <chkbtn name="check_show_sum" title="汇总显示" skin="IDB_CHECK_BOX" pos="16,-25" width="100" height="15" 

	<button name="collateral_order.refresh" title="" pos="-186,-25" width="70" height="25" skin="IDB_BT_REFRESH_small" tip="刷新" show="1" />
	 <button name="collateral_order.revocation" title="" pos="-106,-25" width="70" height="25" skin="IDB_BT_revocation_small" tip="撤销" show="1" />
  

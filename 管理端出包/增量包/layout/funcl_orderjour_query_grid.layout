        <column title="全部费用" width="100" align="right" key="all_fare" data_type="T" />         
        <column title="订单时间" width="100" align="center" key="order_time" data_type="N"/>
         <column title="操作员名称" width="100" align="center" key="operator_name" />
        <column title="申报日期" width="100" align="center" key="report_date" />
        <column title="申报时间" width="100" align="center" key="report_time" data_type="N"/>
				<column title="资产账户" width="100" align="center" key="asset_account" />
        <column title="股东代码" width="100" align="center" key="stock_account" />
        <column title="市场编号" width="100" align="center" key="exchange_no" />
 				<column title="操作员编号" width="100" align="center" key="operator_no" />
    </gridctrl>
  </div>

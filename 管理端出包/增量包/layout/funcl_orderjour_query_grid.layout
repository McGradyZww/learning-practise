<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
  <div name="funcl_orderjour_query_grid">
    <img pos="0,0,-0,-0" image="skins\default\MsgBack.png" mode="extrude" framesize="0" show="1" response="0"/>
    <img pos="1,0,-1,3" image="skins\default\line_blue.png" mode="tile" show="1" response="0"/>
		<!--市场编号-->
		<text crtext="000000" crmark="800000" align="right" pos="10,20,90,40" title="市场编号:" font="bold3" show="1"/>
		<combobox name="orderjour_query.exchange_no" tabstop="1" skin="IDB_EDIT" pos="90,12" width="120" height="36" tip="市场编号"
				  small-image="skin:IDB_DROP_DOWN_small"
				  multiline="0" readonly="1" maxchar="50"
					font="default3" crtext="0,0,0" crtip="128,128,128">
		</combobox>
	
   <!--证券代码-->
	<text crtext="000000" crmark="800000" align="right" pos="230,20,310,40" title="证券代码:" font="bold3" show="1"/>
	<edit name="orderjour_query.stock_code" tabstop="1" skin="IDB_EDIT" pos="310,12" width="100" height="36"
		   multiline="0" number="0" readonly="0" maxchar="50" font="default" crtext="0,0,0" stockcode="1" changednotify="1" maxchar="6"/>
		   
	 
	   <!--单元-->
	   <text crtext="000000" crmark="800000" align="right" pos="420,20" width="80" height="25" title="交易单元:" font="bold3" show="1"/>
    <combobox name="orderjour_query.unit_code" tabstop="1" skin="IDB_EDIT" pos="505,12" width="120" height="36" tip="交易单元"
				  small-image="skin:IDB_DROP_DOWN_small"
				  multiline="0" readonly="1" maxchar="50"
					font="default3" crtext="0,0,0" crtip="128,128,128">
	  </combobox>
	  
	  <!--日期-->
		<text crtext="000000" crmark="800000" align="right" pos="630,20,680,40" title="日期:" font="bold3" show="1"/>
		<nativewnd name="orderjour_query.begin_date" tabstop="1" skin="IDB_EDIT" pos="680,12" width="120" height="36"
			   multiline="0" number="0" readonly="0" maxchar="8" font="default" crtext="0,0,0" />
		<text crtext="000000" crmark="800000" align="right" pos="800,20,820,40" title="——" font="bold3" show="1"/>
		<nativewnd name="orderjour_query.end_date" tabstop="1" skin="IDB_EDIT" pos="820,12" width="120" height="36"
			   multiline="0" number="0" readonly="0" maxchar="8" font="default" crtext="0,0,0" />
			   
    <button name="queryorderjour_button.query" title="" pos="950,15" width="80" height="30" skin="IDB_BT_QUERY" tip="查询" show="1" shortcut="F5" enabledirkey="1"/>
    <button name="unitstock_button.refresh" title="" pos="-186,15" width="70" height="30" skin="IDB_BT_REFRESH" tip="刷新" show="0" />
	  <button name="query_button.export" title="" pos="-106,15" width="70" height="30" skin="IDB_BT_EXPORT" tip="导出" show="1" />
	<img pos="1,54,-1,56" image="skins\default\line_split.png" mode="tile" show="1" />	
   <!--结果集grid-->
	<gridctrl name="grid.funcl_orderjour_query" pos="2,62,-2,-2" show="1"
        img-scroll="skin:IDB_SCROLL_V" img-scrollh="skin:IDB_SCROLL_H" img-sep="skin:IDB_LINE_RP"
        img-check="skin:IDB_CHECK_BOX" img-colsep=""

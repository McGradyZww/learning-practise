<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
  <div name="funcl_command_baskettrading_management_order">
    <img pos="0,0,-0,-0" image="skins\default\MsgBack.png" mode="extrude" framesize="0" show="1" response="0"/>
    <!--<img pos="1,0,-1,3" image="skins\default\line_blue.png" mode="tile" show="1" response="0"/>-->
    <!--<text crtext="808080" crmark="800000" align="left" pos="2,5,-15,25" title="  订单查询" font="big" show="1" />-->
   
    <gridctrl name="grid.funcl_basketcommand_order_query" pos="2,5,-2,-45" show="1"
        img-scroll="skin:IDB_SCROLL_V" img-scrollh="skin:IDB_SCROLL_H" img-sep="skin:IDB_LINE_RP"
        img-check="skin:IDB_CHECK_BOX" img-colsep="skin:IDB_LINE_SPLIT"  gird-tip="1"
        img-count="6" image="skin:IDB_LIST_IMG"   sort="1"   
        left-pos="0" row-height="35" header-height="38" bk-transparent="10"
        font-title="big" font="default2" editcolumn="1"
        valign="middle" down-row="1" align="center"  primary-key="batch_no"  
        crtext="255,64,64,64" crhover="0,0,0" crpush="212,234,254" crhead="23,69,144" crrowhover="236,240,254">     
        <column title="批号" width="100" align="center" key="batch_no" />   
				<column title="状态" width="100" align="center" key="order_status" />
        <column title="证券代码" width="100" align="center" key="stock_code"  />
        <column title="证券名称" width="100" align="center" key="stock_name" />
        <column title="订单方向" width="100" align="center" key="order_dir" /> 
		 		<column title="订单数量" width="100" align="right" key="order_qty" data_type="T"  sum_column="1" />		 
        <column title="订单价格" width="100" align="center" key="order_price" data_type="N" />
        <column title="订单金额" width="100" align="right" key="execution_amt" data_type="T" sum_column="1" />
        <column title="成交数量" width="100" align="right" key="execution_qty" data_type="T" sum_column="1" />     
        <column title="成交价格" width="100" align="center" key="execution_price" data_type="N" /> 
        <column title="撤单数量" width="100" align="right" key="cancel_qty" data_type="T" sum_column="1"  />
		<column title="废单原因" width="100" align="center" key="response_info" />
    </gridctrl>
    <gridctrl name="grid.funcl_basketcommand_order_query_total" pos="2,-45,-2,-0" show="1"       
        img-check="skin:IDB_CHECK_BOX" img-colsep="skin:IDB_LINE_SPLIT"  gird-tip="1"
        img-count="6" image="skin:IDB_LIST_IMG"   sort="1"   
        left-pos="0" row-height="35" header-height="38" bk-transparent="10"
        font-title="big" font="default2" editcolumn="1"
        valign="middle" down-row="1" align="center"  primary-key="batch_no"  
        crtext="255,64,64,64" crhover="0,0,0" crpush="212,234,254" crhead="23,69,144" crrowhover="236,240,254">     
        <column title="" width="100" align="center" key="batch_no" />   
				<column title="" width="100" align="center" key="order_status" />
        <column title="" width="100" align="center" key="stock_code"  />
        <column title="" width="100" align="center" key="stock_name" />
        <column title="" width="100" align="center" key="order_dir" /> 
		 		<column title="" width="100" align="right" key="order_qty"   />		 
        <column title="" width="100" align="center" key="order_price"  />
        <column title="" width="100" align="right" key="execution_amt"  />
        <column title="" width="100" align="right" key="execution_qty" />     
        <column title="" width="100" align="center" key="execution_price" /> 
        <column title="" width="100" align="right" key="cancel_qty"  />
		<column title="" width="100" align="center" key="reponse_info" />
    </gridctrl>
	
	
  </div>

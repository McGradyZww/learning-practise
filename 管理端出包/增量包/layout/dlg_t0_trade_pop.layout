			  <img pos="3,0,-3,-3" image="skins\default\MsgBack.png" mode="extrude" framesize="0" show="1" response="0"/>
				<!--交易策略区-->
		    <div name="T+0trade_stock_exchange_pop" pos="10,2,380,-10" placement="T+0trade_stock_exchange_pop"/>
		    <!--分割线-->
        <img pos="378,0,381,-3" image="skins\default\line_blue.png" mode="tile" show="1" response="0"/>
		    <tabctrl name="tab_t0_trade_pop" pos="384,2,-3,-3" img-sep="" img-hover="" img-tabbtn-="skin:IDB_TAB_CLOSE" tabbtnpos-="-21,3"
				font="bold2" item-width="130" tab-height="30" crtext="ffffff" crtexttDeactive="191919" animate="0" >

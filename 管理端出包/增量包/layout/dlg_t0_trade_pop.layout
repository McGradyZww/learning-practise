<?xml version="1.0" encoding="UTF-8" standalone="no" ?>

<dlg name="dlg_t0_trade_pop" title="[APP_NAME]" width="955" height="300"  appwin="1" resize="1" frame="" bkimg="" crbk="3c5680">

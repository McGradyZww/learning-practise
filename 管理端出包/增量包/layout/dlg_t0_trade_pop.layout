		<img pos="0,0,-0,30" name="image.title" image="skins\default\dlg_header.png" mode="titl" show="1" response="0" />
		<text name="user.title" crtext="FFFFFF" crmark="800000" crshadow="808080" font="big" pos="50,4" width="120" height="21" title="[APP_NAME]" response="0" show="1" drag="1"/>
		<button name="button.close" pos="-34,0,-0,30" skin="IDB_BT_CLOSE" show="1" tip="关闭" shortcut="ESC" />
	</base>

	<body>
			<div name="to_trade_pop.body" pos="0,32,-0,-0">	

<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
  <div name="funcl_commandjour_query_grid">
    <img pos="0,0,-0,-0" image="skins\default\MsgBack.png" mode="extrude" framesize="0" show="1" response="0"/>
    <img pos="1,0,-1,3" image="skins\default\line_blue.png" mode="tile" show="1" response="0"/>
		<!--市场编号-->
		<text crtext="000000" crmark="800000" align="right" pos="10,20,90,40" title="市场编号:" font="bold3" show="1"/>
		<combobox name="commandjour_query.exchange_no" tabstop="1" skin="IDB_EDIT" pos="90,12" width="120" height="36" tip="市场编号"
				  small-image="skin:IDB_DROP_DOWN_small"
				  multiline="0" readonly="1" maxchar="50"
					font="default3" crtext="0,0,0" crtip="128,128,128">
		</combobox>
	
   <!--证券代码-->
	<text crtext="000000" crmark="800000" align="right" pos="230,20,310,40" title="证券代码:" font="bold3" show="1"/>
	<edit name="commandjour_query.stock_code" tabstop="1" skin="IDB_EDIT" pos="310,12" width="100" height="36"
		   multiline="0" number="0" readonly="0" maxchar="50" font="default" crtext="0,0,0" stockcode="1" changednotify="1" maxchar="6"/>
		   
	 
	   <!--单元-->
	   <text crtext="000000" crmark="800000" align="right" pos="420,20" width="80" height="25" title="交易单元:" font="bold3" show="1"/>
    <combobox name="commandjour_query.unit_code" tabstop="1" skin="IDB_EDIT" pos="505,12" width="120" height="36" tip="交易单元"
				  small-image="skin:IDB_DROP_DOWN_small"
				  multiline="0" readonly="1" maxchar="50"
					font="default3" crtext="0,0,0" crtip="128,128,128">
	  </combobox>
	   <!--查询方式-->
	   <text crtext="000000" crmark="800000" align="right" pos="610,20" width="80" height="25" title="查询方式:" font="bold3" show="1"/>
    <combobox name="commandjour_query.query_type" tabstop="1" skin="IDB_EDIT" pos="695,12" width="170" height="36" tip="查询方式"
				  small-image="skin:IDB_DROP_DOWN_small"
				  multiline="0" readonly="1" maxchar="100"
					font="default3" crtext="0,0,0" crtip="128,128,128">
	  </combobox>
	  
	  <!--日期-->
		<text crtext="000000" crmark="800000" align="right" pos="850,20,900,40" title="日期:" font="bold3" show="1"/>
		<nativewnd name="commandjour_query.begin_date" tabstop="1" skin="IDB_EDIT" pos="900,12" width="120" height="36"
			   multiline="0" number="0" readonly="0" maxchar="8" font="default" crtext="0,0,0" />
		<text crtext="000000" crmark="800000" align="right" pos="1020,20,1030,40" title="——" font="bold3" show="1"/>
		<nativewnd name="commandjour_query.end_date" tabstop="1" skin="IDB_EDIT" pos="1040,12" width="120" height="36"
			   multiline="0" number="0" readonly="0" maxchar="8" font="default" crtext="0,0,0" />
			   
    <button name="querycommandjour_button.query" title="" pos="1170,15" width="80" height="30" skin="IDB_BT_QUERY" tip="查询" show="1" shortcut="F5" enabledirkey="1"/>
    <button name="unitstock_button.refresh" title="" pos="-186,15" width="70" height="30" skin="IDB_BT_REFRESH" tip="刷新" show="0" />
	  <button name="query_button.export" title="" pos="-106,15" width="70" height="30" skin="IDB_BT_EXPORT" tip="导出" show="1" />
	<img pos="1,54,-1,56" image="skins\default\line_split.png" mode="tile" show="1" />	
   <!--结果集grid-->
	<gridctrl name="grid.funcl_commandjour_query" pos="2,62,-2,-30" show="1"
        img-scroll="skin:IDB_SCROLL_V" img-scrollh="skin:IDB_SCROLL_H" img-sep="skin:IDB_LINE_RP"
        img-check="skin:IDB_CHECK_BOX" img-colsep=""
        img-count="6" image="skin:IDB_LIST_IMG" editcolumn="1"
        left-pos="0" row-height="35" header-height="38" bk-transparent="10"
        font-title="big" font="default2" sort="1" img-colsep="skin:IDB_LINE_SPLIT" 
        valign="middle" down-row="1" align="center" bCellColor="0"
        crtext="255,64,64,64"  crhover="0,0,0" crpush="212,234,254" crhead="23,69,144" crrowhover="236,240,254"> 
        <column title="指令日期" width="100" align="center" key="command_date" />       
        <column title="指令序号" width="100" align="center" key="command_id" data_type="N" />                 
        <column title="指令批号" width="100" align="center" key="batch_no" data_type="N" />
         <column title="产品名称" width="100" align="center" key="product_name" />         
       
        <column title="市场编号" width="100" align="center" key="exchange_no" />
        <column title="资产类型" width="100" align="center" key="asset_type" />
        <column title="证券代码" width="100" align="center" key="stock_code" />
        <column title="证券名称" width="100" align="center" key="stock_name" />
        <column title="指令方向" width="100" align="center" key="command_dir" />
        <column title="指令限价" width="100" align="center" key="limit_price" data_type="N" />
        <column title="指令数量" width="100" align="right" key="command_qty" data_type="T" sum_column="1" />
        <column title="订单数量" width="100" align="right" key="order_qty" data_type="T" sum_column="1" />
        <column title="指令金额" width="100" align="right" key="command_amt" data_type="T" sum_column="1" />       
        <column title="指令状态" width="100" align="center" key="command_status" />
        <column title="成交均价" width="100" align="right" key="busi_average_price" data_type="N" />
        <column title="成交数量" width="100" align="right" key="execution_qty" data_type="T" sum_column="1" />
        <column title="成交金额" width="100" align="right" key="execution_amt" data_type="T" sum_column="1" />
        <column title="成交状态" width="100" align="center" key="execution_status" />
        <column title="起始日期" width="100" align="center" key="command_begin_date" />
        <column title="起始时间" width="100" align="center" key="command_begin_time" data_type="N"/>
        <column title="结束日期" width="100" align="center" key="command_end_date" />
        <column title="结束时间" width="100" align="center" key="command_end_time" data_type="N"/>
        <column title="指令下达人" width="100" align="center" key="command_operator" />
        <column title="指令执行人" width="100" align="center" key="command_executor" />       
        <column title="指令时间" width="100" align="center" key="command_time" data_type="N"/>      
        <column title="交易单元名称" width="100" align="center" key="unit_name" />
        <column title="产品编码" width="100" align="center" key="product_code" />
        <column title="交易单元" width="100" align="center" key="unit_code" />
    </gridctrl>
    <gridctrl name="grid.funcl_commandjour_query_total" pos="2,-30,-2,-0" show="1"
           img-sep="skin:IDB_LINE_RP" 
         modify-column-width="1" column-sep="1" 
        img-colsep="skin:IDB_LINE_SPLIT"       
        left-pos="0" row-height="30" header-height="30" bk-transparent="10"
        font-title="big" font="default2" sort="1" editcolumn="1" 
        valign="middle" down-row="1" align="center"
        crtext="255,64,64,64" crhover="0,0,0" crpush="212,234,254" crhead="23,69,144" crrowhover="236,240,254"> 
        <column title="合计" width="100" align="center" key="command_date" />       
        <column title="" width="100" align="center" key="command_id" data_type="N" />                 
        <column title="" width="100" align="center" key="batch_no" data_type="N" />
         <column title="" width="100" align="center" key="product_name" />         
       
        <column title="" width="100" align="center" key="exchange_no" />
        <column title="" width="100" align="center" key="asset_type" />
        <column title="" width="100" align="center" key="stock_code" />
        <column title="" width="100" align="center" key="stock_name" />
        <column title="" width="100" align="center" key="command_dir" />
        <column title="" width="100" align="center" key="limit_price" data_type="N" />
        <column title="" width="100" align="right" key="command_qty" data_type="T" />
        <column title="" width="100" align="right" key="order_qty" data_type="T" />
        <column title="" width="100" align="right" key="command_amt" data_type="T" />       
        <column title="" width="100" align="center" key="command_status" />
        <column title="" width="100" align="right" key="execution_price" data_type="N" />
        <column title="" width="100" align="right" key="execution_qty" data_type="T" />
        <column title="" width="100" align="right" key="execution_amt" data_type="T" />
        <column title="" width="100" align="center" key="execution_status" />
        <column title="" width="100" align="center" key="command_begin_date" />
        <column title="" width="100" align="center" key="command_begin_time" />
        <column title="" width="100" align="center" key="command_end_date" />
        <column title="" width="100" align="center" key="command_end_time" />
        <column title="" width="100" align="center" key="command_operator" />
        <column title="" width="100" align="center" key="command_executor" />
        
        <column title="" width="100" align="center" key="command_time" />
       
         <column title="" width="100" align="center" key="unit_name" />
         <column title="" width="100" align="center" key="product_code" />
         <column title="" width="100" align="center" key="unit_code" />
    </gridctrl>
  </div>

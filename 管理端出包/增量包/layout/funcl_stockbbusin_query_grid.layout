        <column title="订单日期" width="100" align="center" key="order_date" />
        <column title="订单时间" width="100" align="center" key="order_time" data_type="N" />
        <column title="申报编号" width="100" align="center" key="report_no" data_type="N" />
        <column title="资产账户" width="0" align="center" key="asset_account"  />
        <column title="股东代码" width="0" align="center" key="stock_account"  />
        <column title="市场编号" width="100" align="center" key="exchange_no"  deletable="0" />
        <column title="证券类型" width="100" align="center" key="stock_type" />
        <column title="成交日期" width="100" align="center" key="execution_date" data_type="N" />

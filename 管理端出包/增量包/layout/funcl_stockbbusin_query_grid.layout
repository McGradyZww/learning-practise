        <column title="成交编号" width="100" align="center" key="execution_no" data_type="N"  />
        <column title="产品编码" width="100" align="center" key="product_code" deletable="0" />
        <column title="交易单元" width="100" align="center" key="unit_code" deletable="0" />
		<column title="业务操作员" width="100" align="left" key="busin_operator_name" deletable="0" />
       
    </gridctrl>
    
    	<gridctrl name="grid.funcl_stockbbusin_query_total" pos="2,-30,-0,-0" show="1"

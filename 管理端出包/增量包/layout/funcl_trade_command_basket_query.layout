        <column title="买卖方向" width="100" align="center" key="order_dir" /> 
        <column title="订单价格" width="100" align="center" key="order_price" data_type="N" />
        <column title="订单数量" width="100" align="right" key="order_qty" data_type="N"  />
	    <column title="成交数量" width="100" align="right" key="execution_qty" data_type="N" />

<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
   <div name="funcl_bond_collateral_command_input" >
       <img pos="0,0,-0,-0" image="skins\default\MsgBack.png" mode="extrude" framesize="0" show="1" response="0"/>

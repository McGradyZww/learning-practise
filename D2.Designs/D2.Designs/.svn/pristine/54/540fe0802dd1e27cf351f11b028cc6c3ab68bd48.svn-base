    int v_update_time;
    int v_init_date;
    char v_crncy_type[4];
    double v_begin_amt;
    double v_first_amt;
    double v_curr_amt;
    double v_pd_dist_amt;
    double v_pd_auth_amt;
    double v_amt_check_diff;
    double v_pre_settle_amt;
    double v_total_cash_divide;
    double v_total_pre_fee;
    double v_total_rece_amt;
    double v_total_payab_amt;
    double v_intrst_cacl_amt;
    double v_pre_entry_intrst_amt;

        /* set @可用金额# =@可用金额# +@盯市盈亏#; */
        set v_avail_amt =v_avail_amt +v_marked_pandl;

        /* 调用【原子_产品期货_资金_更新交易资金】*/
        call db_pdfutu.pra_pdfuca_UpdateTrdCapital(

    label_pro:BEGIN
    

    /* [事务开始] */
    START TRANSACTION;


        /* 调用【原子_产品期货_持仓_计算资产账户盯市盈亏】*/
        call db_pdfutu.pra_pdfupo_CalcAsacMarkedPandl(
